{
  "name": "Ambit Energy Scraper",
  "version": "1.0.0",
  "slug": "ambit_energy_scraper",
  "description": "Scrapes energy consumption data from Ambit Energy and sends it to Home Assistant.",
  "startup": "application",
  "boot": "auto",
  "options": {
    "username": "your_username",
    "password": "your_password",
    "ha_url": "http://home_assistant_ip:8123/api/states/sensor.ambit_energy_consumption",
    "ha_token": "YOUR_LONG_LIVED_ACCESS_TOKEN"
  },
  "schema": {
    "username": "str",
    "password": "str",
    "ha_url": "str",
    "ha_token": "str"
  },
  "image": "local/ambit_energy_scraper",
  "arch": [
    "armv7",
    "armhf",
    "aarch64",
    "amd64"
  ],
  "ports": {},
  "map": [
    "config",
    "ssl",
    "backup"
  ],
  "environment": {},
  "privileged": false,
  "devices": [],
  "host_network": false,
  "workdir": "/app"
}

