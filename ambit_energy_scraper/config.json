{
  "name": "Custom Home Assistant Add-ons",
  "slug": "custom_addons",
  "version": "1.0",
  "url": "https://github.com/lam1623/ambit_energy_scraper",
  "addons": [
    {
      "slug": "ambit_energy_scraper",
      "name": "Ambit Energy Scraper",
      "description": "Scrapes energy consumption data from Ambit Energy and sends it to Home Assistant.",
      "version": "1.0.0",
      "architecture": ["armv7", "armhf", "aarch64", "amd64"],
      "startup": "application",
      "boot": "auto",
      "options": {
        "username": "str",
        "password": "str",
        "ha_url": "str",
        "ha_token": "str"
      },
      "schema": {
        "username": "str",
        "password": "str",
        "ha_url": "str",
        "ha_token": "str"
      },
      "repository": "https://github.com/lam1623/ambit_energy_scraper",
      "url": "https://github.com/lam1623/ambit_energy_scraper/tree/main/ambit_energy_scraper",
      "image": "local/ambit_energy_scraper",
      "logo": "https://github.com/lam1623/ambit_energy_scraper/blob/main/ambit_energy_scraper/logo.png?raw=true",
      "documentation": "https://github.com/lam1623/ambit_energy_scraper/blob/main/ambit_energy_scraper/README.md",
      "map": ["config", "ssl", "backup"],
      "environment": {
        "USERNAME": "secret.ambit_username",
        "PASSWORD": "secret.ambit_password",
        "HA_URL": "http://home_assistant_ip:8123/api/states/sensor.ambit_energy_consumption",
        "HA_TOKEN": "secret.ha_token"
      },
      "privileged": false,
      "devices": [],
      "ports": {},
      "host_network": false,
      "workdir": "/app"
    }
  ]
}

